  timeout = .05;
}

if (timeout){
  if (this.demo == true){
    if (this.mode == 0){
      this.percent += 0.01;
      if (this.percent > 1){
        this.mode = 1;
        this.percent = 1;
      }
    }elseif(this.mode == 1){
      this.percent -= 0.01;
      if (this.percent < 0){
        this.mode = 0;
        this.percent = 0;
      }
    }
    draw_circle();
  }else{
    this.percent = (playerhearts/playerfullhearts);
    if (this.percent != this.old_percent){
      this.old_percent = this.percent;
      draw_circle();
    }
  }
  timeout = 0.05;
}

function init(){
  setarray this.bx,this.detail+1;
  setarray this.by,this.detail+1;
  setarray this.gx,this.detail+1;
  setarray this.gy,this.detail+1;
  setarray this.angle,this.detail;
  for (this.i = 1;this.i < this.detail+1;this.i++){
    this.angle[this.i] = (pi*2)*(this.i/this.detail);
    this.gx[this.i] = this.x+(sin(this.angle[this.i])*this.rad);
    this.gy[this.i] = this.y+(cos(this.angle[this.i])*this.rad);
    this.bx[this.i] = this.x+(sin(this.angle[this.i]-this.angle[1])*this.rad);
    this.by[this.i] = this.y+(cos(this.angle[this.i]-this.angle[1])*this.rad);
  }
}

function draw_circle(){
  hideimgs 0,this.detail+2;
  for (this.i = 1;this.i < (this.detail*this.percent)+1 ;this.i++){
    showpoly this.i,{this.x,this.y,this.gx[this.i],this.gy[this.i],this.bx[this.i],this.by[this.i]};
    changeimgcolors this.i,(this.i/this.detail),(this.i/this.detail),(this.i/this.detail),1;
    changeimgvis this.i,5;
  }
  if (this.demo == true){
    showimg this.i+1,@#v(this.i-1)/#v(this.detail) Poly's,25,25;
    changeimgvis this.i+1,6;
  }
}
