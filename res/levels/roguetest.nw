function draw_outline(){
  this.x = strtofloat(#I(client.window_#v(this.selected),win_x));
  this.y = strtofloat(#I(client.window_#v(this.selected),win_y));
  this.w = strtofloat(#I(client.window_#v(this.selected),win_width))+16;
  this.h = strtofloat(#I(client.window_#v(this.selected),win_height))+16;
  while (this.w%16!=0)this.w++;
  while (this.h%16!=0)this.h++;
  if (this.w < this.min_width)this.w=this.min_width;
  if (this.h < this.min_height)this.h=this.min_height;
  if (this.w > this.max_width)this.w=this.max_width;
  if (this.h > this.max_height)this.h=this.max_height;
  if (this.x+this.w>screenwidth)this.x = this.x-(this.x+this.w-screenwidth);
  if (this.y+this.h>screenheight)this.y = this.y-(this.y+this.h-screenheight);
  if (this.x<0)this.x=0;
  if (this.y<0)this.y=0;
  showpoly 6000,{this.x,this.y,this.x+this.w,this.y};
  showpoly 6001,{this.x+this.w,this.y,this.x+this.w,this.y+this.h};
  showpoly 6002,{this.x+this.w,this.y+this.h,this.x,this.y+this.h};
